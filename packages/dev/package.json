    "@truffle/hdwallet-provider": "^2.0.11",
    "@types/chai-as-promised": "^7.1.3",
    "@types/sinon": "^9.0.10",
    "@types/sinon-chai": "^3.2.5",
    "abi-decoder": "^2.3.0",
    "async-mutex": "^0.3.2",
    "axios": "^0.27.2",
    "bn.js": "^5.2.1",
    "body-parser": "^1.20.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "ethereumjs-util": "^7.1.0",
    "ethereumjs-wallet": "^1.0.2",
    "ethers": "^5.7.2",
    "express": "^4.18.1",
    "hardhat": "^2.6.8",
    "rlp": "^3.0.0",
    "sinon": "^9.2.3",
    "sinon-chai": "^3.5.0",
    "truffle-plugin-stdjsonin": "github:mhrsalehi/truffle-plugin-stdjsonin",
    "ts-node": "8.6.2",
    "web3": "^1.7.4",
    "web3-core": "^1.7.4",
    "web3-core-helpers": "^1.7.4",
    "web3-eth-contract": "^1.7.4",
    "web3-providers-http": "^1.7.4",
    "web3-utils": "^1.7.4",
    "winston": "^3.8.1"
